{ "compilerOptions": { 
    "rootDir": "./app", 
    "outDir": "./dist", 
    "target": "es6", 
    "lib": ["dom", "dom.iterable", "es2020"], 
    "allowJs": true, 
    "skipLibCheck": true, 
    "strict": true, 
    "noEmit": true, 
    "noImplicitAny": true, 
    "isolatedModules": true, 
    "esModuleInterop": true, 
    "forceConsistentCasingInFileNames": true, 
    "module": "esnext", 
    "moduleResolution": "node", 
    "resolveJsonModule": true, 
    "jsx": "react-jsx", 
    "baseUrl": ".", 
    "paths": { "@/*": ["app/*"] } 
    }, 
    "include": ["app/**/*"], 
    "exclude": ["node_modules"]
 }
